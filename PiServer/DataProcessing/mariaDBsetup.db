CREATE DATABASE job_market_data;
USE job_market_data;
CREATE TABLE current_job_availabilities_data(
    company_name VARCHAR(30) PRIMARY KEY,
    company_department VARCHAR(30),
    job_title VARCHAR(30),
    job_id_number VARCHAR(50) FOREIGN KEY,  # relate to hired!!
    job_url VARCHAR(100),
    job_release_date VARCHAR(30),
    company_industry VARCHAR(30),
    job_location VARCHAR(30),
    employment_type VARCHAR(30),
    job_workplaceType VARCHAR(30),
    experience_level VARCHAR(30),
    years_of_experience VARCHAR(30),
);

